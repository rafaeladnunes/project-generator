{
  "name": "monolithic",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.19.0",
    "express": "^4.16.4",
    "mysql2": "^1.6.5",
    "sequelize": "^5.8.5",
    "swagger-express": "^1.0.5",
    "swagger-express-middleware": "^2.0.2"
  },
  "devDependencies": {
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.17.2",
    "jest": "^24.8.0",
    "nodemon": "^1.19.0",
    "sequelize-cli": "^5.4.0",
    "supertest": "^4.0.2"
  },
  "scripts": {
    "start": "NODE_ENV=production node index.js",
    "start:dev": "NODE_ENV=development nodemon index.js",
    "db:migrate": "sequelize db:migrate --env $NODE_ENV --config src/config/config.json --migrations-path src/migrations/ --models-path src/models/ --seeders-path src/seeders/",
    "db:create": "sequelize db:create --config src/config/config.json --env $NODE_ENV",
    "eslint": "eslint src",
    "test": "NODE_ENV=test jest --detectOpenHandles --forceExit"
  }
}
